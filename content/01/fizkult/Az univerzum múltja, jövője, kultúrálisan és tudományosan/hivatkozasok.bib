@Inbook{doppler,
    author="Andrews, Steven S.",
    title="Doppler Effects, Redshifts, and Blueshifts",
    bookTitle="Light and Waves: A Conceptual Exploration of Physics",
    year={2023},
    publisher="Springer International Publishing",
    pages="101--121",
    abstract="When standing next to a freeway, cars emit a high pitch sound as they approach and then a low pitch sound as they recede. This is an example of the Doppler effect, which is the change of a wave's frequency due to relative motion between the source and observer. It occurs because of the observer's speed through the waves and/or because the source emits waves at different positions due to its speed. Doppler effects are used for medical imaging, environmental fluid flow research, police radar guns, and other applications. They also show how fast stars are moving toward or away from the Earth through shifted light wavelengths that are called redshifts and blueshifts. Extreme Doppler effects arise for objects that move faster than the waves they create. Examples are supersonic airplanes, motorboats that skim across the water, and even underwater high energy particles that move faster than light waves.",
    isbn="978-3-031-24097-3",
    doi="10.1007/978-3-031-24097-3_5",
    url="https://doi.org/10.1007/978-3-031-24097-3_5"
}

